        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(self.name)\n",
        "    print(self.age)\n",
        "\n",
        "#インスタンス化\n",
        "human = Human(\"札幌一郎\", 45)\n",
        "\n",
        "#メソッドにアクセスして実行する\n",
        "human.printinfo()"
      ]
